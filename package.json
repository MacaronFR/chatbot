{
  "name": "imacaron-chatbot",
  "description": "A chatbot open-source library for scenarized chatbot",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "/index.css"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
